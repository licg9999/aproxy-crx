{
  "manifest_version": 2,
  "name": "aproxy-crx",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "./imgs/icon.png",
    "default_popup": "./popup.html",
    "default_title": "AproxyExtension"
  },
  "background": {
      "page": "./background.html"
  },
  "permissions": [
      "background",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
  ]
}
